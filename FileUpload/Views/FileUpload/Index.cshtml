
@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<form method="post" enctype="multipart/form-data" asp-controller="FileUpload" asp-action="Index">

    <div class="form-group">
        <div class="col-md-10">
            <p>Upload File</p>
            <input type="file" name="files" multiple/>
        </div>
    </div>

    <div class="form-group">
        <div class="col-md-10">
           
            <input type="submit" value="Upload"/>
        </div>
    </div>
</form>

